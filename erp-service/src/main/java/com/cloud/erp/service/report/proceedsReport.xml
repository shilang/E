<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="http://www.orbitatech.com/schema/module"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.orbitatech.com/schema/module module.xsd">
	<name>应款单</name>
	<master name="master" class="com.cloud.erp.entities.table.RecProceeds">
		<fields>
			<field title="状态">result</field>
			<field title="单据编号">billNo</field>
			<field title="日期">date</field>
			<field title="核算项目类别">subject</field>
			<field title="核算项目">customerName</field>
			<field title="核算项目开户银行">custBank</field>
			<field title="核算项目银行账号">custBankNum</field>
			<field title="财务日期">settleDate</field>
			<field title="结算方式">settleName</field>
			<field title="币别">currencyName</field>
			<field title="汇率">exchangeRate</field>
			<field title="单据金额">amount</field>
			<field title="实收金额">settleAmount</field>
			<field title="银行费用">bankCost</field>
			<field title="实收币别">settleCurrencyName</field>
			<field title="收款银行">bank</field>
			<field title="账号">bankNum</field>
			<field title="摘要">explanation</field>
			<field title="部门">deptName</field>
			<field title="业务员">employeeName</field>
			<field title="制单">createrName</field>
			<field title="审核人">checkerName</field>
			<field title="审核日期">checkDate</field>
			<field title="源单单号">sourceBillNo</field>
			<field title="源单类型">sourceType</field>
			<field title="备注">remark</field>
		</fields>
	</master>
	<entries>
		<entry name="entries" class="com.cloud.erp.entities.table.RecProceedsEntry">
			<fields>
				<field title="产品代码">number</field>
				<field title="产品名称">itemName</field>
				<field title="规格型号">itemModel</field>
				<field title="单位">itemUnit</field>
				<field title="辅助属性">attrContent</field>
				<field title="数量">quantity</field>
				<field title="单价">price</field>
				<field title="金额">amount</field>
				<field title="结算数量">settleQty</field>
				<field title="结算实收金额">settleAmount</field>
				<field title="源单单号">sourceBillNo</field>
				<field title="备注">remark</field>			
			</fields>	
		</entry>
	</entries>
</module>