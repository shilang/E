  <process id="sales-outstock-process" name="sales outstock process" isExecutable="true">
    <endEvent id="theEnd" name="End"></endEvent>
    <userTask id="salesOutStockCheck1" name="销售出库一级审核" activiti:candidateGroups="物料经理">
      <extensionElements>
        <activiti:formProperty id="result" name="result" type="string" expression="${auditModel.result}"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <userTask id="salesOutStockCommit" name="销售出库提交" activiti:assignee="${initiator}">
      <extensionElements>
        <activiti:formProperty id="taskBusinessType" name="Task Business Type" expression="${auditModel.taskBusinessType}" default="check"></activiti:formProperty>
        <activiti:formProperty id="path" name="path" type="string" expression="${auditModel.path}" default="view/salesMgmt/salesOutStock/salesOutStockEditDlg.jsp"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="salesOutStockCheck1" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="salesOutStockCommit" targetRef="CommitServiceTask"></sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="CommitServiceTask" targetRef="salesOutStockCheck1"></sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="CheckServiceTask" targetRef="theEnd"></sequenceFlow>
    <sequenceFlow id="flow14" name="通过" sourceRef="exclusivegateway1" targetRef="CheckServiceTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditModel.result=='approve'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="CommitServiceTask" name="Commit Service Task" activiti:class="com.cloud.erp.activiti.CommitJavaDelegate">
      <extensionElements>
        <activiti:executionListener event="end" class="com.cloud.erp.activiti.MessageJavaDelegate">
          <activiti:field name="msg">
            <activiti:string><![CDATA[已提交]]></activiti:string>
          </activiti:field>
        </activiti:executionListener>
      </extensionElements>
    </serviceTask>
    <serviceTask id="CheckServiceTask" name="Check Service Task" activiti:class="com.cloud.erp.activiti.CheckJavaDelegate">
      <extensionElements>
        <activiti:executionListener event="end" class="com.cloud.erp.activiti.MessageJavaDelegate">
          <activiti:field name="msg">
            <activiti:string><![CDATA[已审核]]></activiti:string>
          </activiti:field>
        </activiti:executionListener>
      </extensionElements>
    </serviceTask>
    <serviceTask id="CancelCheckServiceTask" name="Cancel Check Service Task" activiti:class="com.cloud.erp.activiti.CancelCheckJavaDelegate">
      <extensionElements>
        <activiti:executionListener event="end" class="com.cloud.erp.activiti.MessageJavaDelegate">
          <activiti:field name="msg">
            <activiti:string><![CDATA[已驳回]]></activiti:string>
          </activiti:field>
        </activiti:executionListener>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask4" name="Update Relation Service Task" activiti:class="com.cloud.erp.activiti.UpdateRelationJavaDelegate"></serviceTask>
    <sequenceFlow id="flow16" sourceRef="servicetask4" targetRef="salesOutStockCommit"></sequenceFlow>
    <userTask id="salesOutStockCheck2" name="销售出库二级审核" activiti:candidateGroups="销售经理">
      <extensionElements>
        <activiti:formProperty id="result" name="result" type="string" expression="${auditModel.result}"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow17" sourceRef="salesOutStockCheck2" targetRef="exclusivegateway1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow19" name="通过" sourceRef="exclusivegateway2" targetRef="salesOutStockCheck2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditModel.result=='approve'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow21" sourceRef="CancelCheckServiceTask" targetRef="salesOutStockCommit"></sequenceFlow>
    <startEvent id="theStart" name="Start" activiti:initiator="initiator"></startEvent>
    <sequenceFlow id="flow23" sourceRef="theStart" targetRef="servicetask4"></sequenceFlow>
    <sequenceFlow id="flow24" name="驳回" sourceRef="exclusivegateway2" targetRef="CancelCheckServiceTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditModel.result=='reject'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow25" name="驳回" sourceRef="exclusivegateway1" targetRef="CancelCheckServiceTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditModel.result=='reject'}]]></conditionExpression>
    </sequenceFlow>
  </process>
